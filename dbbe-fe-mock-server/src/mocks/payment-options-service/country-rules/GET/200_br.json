{
  "name":"Brazil",
  "code":"BR",
  "currency":"BRL",
  "reachability":[

  ],
  "iban":{
    "supported":true,
    "required":false
  },
  "accountNumber":{
    "supported":true,
    "mandatoryForDomesticCurrency":false
  },
  "bankcode":{
    "supported":false,
    "mandatoryForDomesticCurrency":false
  },
  "bic":{
    "required":false,
    "unsupportedWhenAccountNumber":false
  },
  "formatting":[
    {
      "fieldId":"name",
      "fieldLabel":"Name",
      "validation":"(?=^.{0,35}$)([A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*[A-Za-z0-9]+[A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*)"
    },
    {
      "fieldId":"reference",
      "fieldLabel":"Message for payee (optional)",
      "validation":"^[A-Za-z0-9 \\/\\?\\(\\)\\.,\\-'\\+]{0,18}$"
    },
    {
      "fieldId":"accountHolderStreetName",
      "fieldLabel":"Street name",
      "validation":"(?=^.{0,35}$)([A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*[A-Za-z0-9]+[A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*)"
    },
    {
      "fieldId":"accountHolderAddressLine1",
      "fieldLabel":"Address line 1",
      "validation":"(?=^.{0,35}$)([A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*[A-Za-z0-9]+[A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*)"
    },
    {
      "fieldId":"accountHolderAddressLine2",
      "fieldLabel":"Address line 2",
      "validation":"(?=^.{0,35}$)([A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*[A-Za-z0-9]+[A-Za-z0-9\\/\\?\\(\\)\\.,\\-'\\+ ]*)"
    },
    {
      "fieldId":"accountHolderPostCode",
      "fieldLabel":"Postal code",
      "validation":"^[A-Za-z0-9 \\-']{0,12}$"
    },
    {
      "fieldId":"iban",
      "fieldLabel":"IBAN",
      "validation":"[a-zA-Z]{2}[0-9]{2}[a-zA-Z0-9]{4}[0-9]{7}([a-zA-Z0-9]?){0,16}"
    },
    {
      "fieldId":"bic",
      "fieldLabel":"BIC",
      "validation":"([a-zA-Z]{4})([a-zA-Z]{2})(([2-9a-zA-Z]{1})([0-9a-np-zA-NP-Z]{1}))((([0-9a-wy-zA-WY-Z]{1})([0-9a-zA-Z]{2}))|([xX]{3})|)"
    },
    {
      "fieldId":"accountNumber",
      "fieldLabel":"Account number",
      "validation":"^([a-zA-Z0-9-:\\(\\)\\?\\/]){1,34}$"
    },
    {
      "fieldId":"bankCode",
      "fieldLabel":"Bank code",
      "validation":"^([0-9]){0,11}$"
    }
  ]
}
